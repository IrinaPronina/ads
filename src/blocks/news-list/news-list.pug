//- Все примеси в этом файле должны начинаться c имени блока (news-list)

mixin news-list(text, mods)

  //- Принимает:
  //-   text    {string} - текст
  //-   mods    {string} - список модификаторов
  //- Вызов:
        +news-list('Текст', 'some-mod')

  -
    // список модификаторов
    var allMods = '';
    if(typeof(mods) !== 'undefined' && mods) {
      var modsList = mods.split(',');
      for (var i = 0; i < modsList.length; i++) {
        allMods = allMods + ' news-list--' + modsList[i].trim();
      }
    }

  .news-list(class=allMods)&attributes(attributes)
